import java.util.Arrays;

public class Sort{
  public String sort(String s){
    char[] ch = s.toCharArray();
    Arrays.sort(ch);
    return new String(ch);
  }
  public boolean permutation(String s, String t){
    if(s.length()!=t.length()){
      return false;
    }
    return sort(s).equals(sort(t));
  }
}
