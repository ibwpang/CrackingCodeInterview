public void replacespaces(char[] s, int length){
  int spacecount=0;
  int newlength=0;
  int i=0;
  for(i=0;i<s.length;i++){
    if(str[i]==' '){
      spacecount++;
    }
  }
  newlength = length+spacecount*2;
  str[newlength]='\0';
  for(i = length-1;i>=0;i--){
    if(str[i]==' '){
      str[newlength-1]='0';
      str[newlength-1]='2';
      str[newlength-1]='%';
      newlength=newlength-3;
    }
    else{
      str[newlength-1]=str[i];
      newlength--;
    }
  }
}
